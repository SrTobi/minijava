class A {

	public int x;

	public static void main(String[] args) {
	    A a = new A();
	    a.x = 0;
	    System.out.println(a.foo());
	    System.out.println(a.x);
		return;
	}

	public int foo() {
	    this.x = 5;
	    int bar = 4;
	    this.x = bar;
	    bar = this.x;
	    x = 10;
		return 0 + this.x + x + bar;
	}
}
