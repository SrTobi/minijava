class Test {

	public int select(boolean p, int a, int b) {
		if (p) { return b; } else { return a; }
	}

	public static void main(String[] args) {
		Test test = new Test();
		boolean bot = (System.id(0) != 0);
		boolean top = (System.id(1) != 0);
		System.out.println(test.select(false, 1, 2));
		System.out.println(test.select(true, 3, 4));
	}

}
